{
    "name": "miguelalcaino/mindbody-api-client",
    "description": "Php client for Mindbody",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Alcaino",
            "email": "miguel.alcaino@ogb.cl"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.5",
        "jms/serializer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "MiguelAlcaino\\MindbodyApiClient\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiguelAlcainoTest\\MindbodyApiClient\\Unit\\": "./tests/unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
