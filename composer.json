{
    "name": "miguelalcaino/mindbody-api-client",
    "description": "Php client for Mindbody",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Alcaino",
            "email": "miguel.alcaino@ogb.cl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.4",
        "jms/serializer": "^3.4",
        "vlucas/phpdotenv": "^5.2",
        "psr/cache": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MiguelAlcaino\\MindbodyApiClient\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiguelAlcainoTest\\MindbodyApiClient\\Test\\Unit\\": "./tests/unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^2.1"
    }
}
